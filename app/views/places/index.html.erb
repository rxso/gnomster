<%= @places.each do |place| %>
  <div class="booyah-box col-xs-6 col-xs-offset-3">
    <h1><%= place.name %></h1>
    <h5><%= place.address %></h5><br  /><br  />
    <p><%= place.description %></p><br  />
  </div>

<% end %>

<!--This adds the page index at the bottom -->
<div class="pages col-xs-6 col-xs-offset-3">
  <%= will_paginate @places %>
</div>
